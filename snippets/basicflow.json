{
  "BasifFlow": {
    "prefix": "ktbf",
    "body": [
    "$1intents:",
      "\tintent_test:",
        "\t\tinitial: true",
        "\t\ttype: \"text\"",
        "\t\tcondition: \"content == 'test'\"",
    "states: ",
      "\tstate_initial:",
        "\t\tinitial: true",
        "\t\ttransitions:",
          "\t\t\tstate_test:",
            "\t\t\t\tcondition: \"intent == 'intent_test'\"",
      "\tstate_test:",
        "\t\tend: true",
        "\t\taction:",
          "\t\t\t- name: \"act_test\"",
    "actions:",
      "\tact_test:", 
        "\t\ttype: \"text\"",
        "\t\toptions:",
          "\t\t\ttext:",
            "\t\t\t\t- \"Hello\"",
            "\t\t\t\t- \"Test\""
    ],
    "description": "Basic flow"
  }
}